*                       text=auto eol=lf

# Windows/Visual Studio overrides
*.bat                   eol=crlf
*.sln                   eol=crlf
*.csproj                eol=crlf
misc/msvs/*             eol=crlf
# Godot text files (keep as text; explicitly disable LFS)
*.tscn                  text eol=lf -filter -diff -merge
*.gd                    text eol=lf -filter -diff -merge
*.gdc                   text eol=lf -filter -diff -merge
*.shader                text eol=lf -filter -diff -merge
*.tres                  text eol=lf -filter -diff -merge
